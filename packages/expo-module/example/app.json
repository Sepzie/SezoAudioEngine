{
  "expo": {
    "name": "Sezo Audio Engine Example",
    "slug": "sezo-audio-engine-example",
    "sdkVersion": "52.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "0.0.1",
    "jsEngine": "hermes",
    "android": {
      "package": "com.anonymous.sezoaudioengineexample",
      "permissions": [
        "RECORD_AUDIO"
      ]
    },
    "ios": {
      "infoPlist": {
        "UIBackgroundModes": [
          "audio"
        ],
        "NSMicrophoneUsageDescription": "Allow access to the microphone for recording audio."
      },
      "bundleIdentifier": "com.anonymous.sezoaudioengineexample"
    }
  }
}

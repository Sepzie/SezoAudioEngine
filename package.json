{
  "name": "sezo-audio-engine",
  "private": true,
  "version": "0.0.1",
  "workspaces": [
    "packages/android-engine",
    "packages/expo-module",
    "packages/expo-module/example"
  ],
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "dev:example": "yarn build:expo-module && yarn build:android-engine && yarn run:example",
    "build:expo-module": "yarn workspace sezo-audio-engine build",
    "build:android-engine": "cd packages/android-engine/android && ./gradlew :engine:build",
    "build:example:android": "yarn workspace sezo-audio-engine-example android",
    "run:example": "yarn workspace sezo-audio-engine-example android",
    "clean": "rm -rf packages/expo-module/dist packages/expo-module/example/android/app/build packages/expo-module/example/android/.gradle",
    "rebuild": "yarn clean && yarn dev:example"
  }
}
